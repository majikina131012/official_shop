<h1>グッズ一覧</h1>

<table>
  <thead>
    <tr>
      <th>グッズ名</th>
      <th><br></th>
    </tr>
  </thead>
<% @items.each do |item| %>
  <tbody>
    <tr>
      <td><%= genre.name %></td>
      <td><%= link_to "編集する", edit_admin_item_path(item.id) %></td>
    </tr>
  </tbody>
<% end %>
</table>

